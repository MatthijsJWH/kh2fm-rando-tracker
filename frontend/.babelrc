// vim: ft=javascript
{
  "plugins": [
    ["transform-inline-environment-variables", {
      "include": [
        "WS_URL",
      ],
    }],
  ],
  "env": {
    "production": {
      "comments": false,
      "presets": [
        "@babel/preset-env",
        ["minify", {"keepFnName": false}],
      ],
    },
    "pretty": {
      "presets": [
        "@babel/preset-env",
      ],
    },
  },
}
